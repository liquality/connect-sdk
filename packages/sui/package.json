{
  "name": "@wormhole-foundation/connect-sdk-sui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "tests"
  },
  "license": "Apache-2.0",
  "version": "0.1.0-beta.1",
  "type": "module",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "types": "./cjs/index.d.ts",
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    }
  },
  "author": "",
  "description": "SDK for SUI, used in conjunction with @wormhole-foundation/connect-sdk",
  "files": [
    "cjs/**/*",
    "esm/**/*",
    "src/**/*"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect",
    "sui"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "clean": "rimraf cjs esm *.tsbuildinfo",
    "lint": "npm run prettier && eslint --fix",
    "prettier": "prettier --write ./src"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@mysten/sui.js": "^0.37.1",
    "@wormhole-foundation/connect-sdk": "*",
    "ethers": "^5.7.2"
  }
}
